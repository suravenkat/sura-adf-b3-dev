						"value": "[parameters('TumblingWindowTrigger_24hrs_properties_pipeline_parameters_startdate')]"
					},
					"TumblingWindowTrigger_24hrs_properties_pipeline_parameters_enddate": {
						"value": "[parameters('TumblingWindowTrigger_24hrs_properties_pipeline_parameters_enddate')]"
					},
					"TumblingWindow_Trigger_properties_pipeline_parameters_startdate": {
						"value": "[parameters('TumblingWindow_Trigger_properties_pipeline_parameters_startdate')]"
